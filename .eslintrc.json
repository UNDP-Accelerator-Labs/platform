{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "max-len": "off",
    "require-jsdoc": "off",
    "no-implicit-globals": "error",
    "no-invalid-this": "off",
    "no-undef": "error",
    "no-extend-native": "off"
  },
  "plugins": ["ejs-js"],
  "ignorePatterns": []
}
