{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "camelcase": "off",
    "max-len": "off",
    "require-jsdoc": "off"
  },
  "plugins": ["ejs-js"],
  "ignorePatterns": [
    "/public/*",
    "/routes/accept/*",
    "/routes/browse/*",
    "/routes/check/*",
    "/routes/contribute/*",
    "/routes/delete/*",
    "/routes/engage/*",
    "/routes/forward/*",
    "/routes/helpers/*",
    "/routes/import/*",
    "/routes/login/*",
    "/routes/logout/*",
    "/routes/mobilize/*",
    "/routes/print/*",
    "/routes/publish/*",
    "/routes/redirect/*",
    "/routes/request/*",
    "/routes/save/*",
    "/routes/scripts/*",
    "/routes/share/*",
    "/views/*"
  ]
}
